  <script>
    $(function(){
      var url = document.URL;
      $('ul.nav.nav-tabs').find('li').find('a').each(function(index, elem){
        if(url.indexOf(elem.href) > -1){
              $('ul.nav.nav-tabs').find('li').each(function(index, elem){
                  $(elem).removeClass("active");
              })
              $(elem).parent().addClass("active");
              return false;
          }
      })
    });
  </script>
